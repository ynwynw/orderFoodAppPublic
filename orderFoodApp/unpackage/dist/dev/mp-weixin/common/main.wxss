button {
  margin: 0;
}
button[type='primary'] {
  background-color: #ADB838;
  color: #ffffff;
  font-size: 28rpx;
}
button[type='primary'][disabled] {
  background-color: #D1D78C;
}
button[type='primary'][plain] {
  color: #ADB838;
  border: 1rpx solid #ADB838;
}
button[type='primary'][plain].button-hover {
  color: rgba(173, 184, 56, 0.5);
  border: 1rpx solid rgba(173, 184, 56, 0.5);
}
button[type='primary'].button-hover {
  background-color: #d5da91;
}
button[type='primary']::after {
  border: 0;
}
button[type='default'][plain] {
  color: #919293;
  border: 1rpx solid #919293;
}
button[type='default'][plain].button-hover {
  color: rgba(145, 146, 147, 0.5);
  border: 1rpx solid rgba(145, 146, 147, 0.5);
}
